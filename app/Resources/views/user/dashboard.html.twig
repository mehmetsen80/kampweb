{% extends '::default.html.twig' %}
{% block body %}
    <body class="ecommerce-page">
    <!-- End: Topbar -->
    <!-- Start: Content-Wrapper -->
    <section id="content_wrapper">
        <!-- Start: Topbar -->
        <header id="topbar" class="ph10">
            <div class="topbar-left">
                <ul class="nav nav-list nav-list-topbar pull-left">
                    <li class="active">
                        <a href="{{ path('dashboard') }}">Dashboard</a>
                    </li>
                </ul>
            </div>
            <div class="topbar-right hidden-xs hidden-sm">
                <a href="ecommerce_orders.html" class="btn btn-default btn-sm light fw600 ml10">
                    <span class="fa fa-plus pr5"></span> New Order</a>
                <a href="ecommerce_products.html" class="btn btn-default btn-sm light fw600 ml10">
                    <span class="fa fa-plus pr5"></span> Add Product</a>
                <a href="ecommerce_customers.html" class="btn btn-default btn-sm light fw600 ml10">
                    <span class="fa fa-user pr5"></span> Add Customer</a>
            </div>
        </header>
        <!-- End: Topbar -->

        <!-- Begin: Content -->
        <section id="content" class="table-layout animated fadeIn">

            <!-- begin: .tray-center -->
            <div class="tray tray-center">

                <!-- dashboard tiles -->
                <div class="row">
                    <div class="col-sm-4 col-xl-3">
                        <div class="panel panel-tile text-center br-a br-grey">
                            <div class="panel-body">
                                <h1 class="fs30 mt5 mbn">{{ events|length }}</h1>
                                <h6 class="text-system">Events</h6>
                            </div>

                        </div>
                    </div>
                    <div class="col-sm-4 col-xl-3">
                        <div class="panel panel-tile text-center br-a br-grey">
                            <div class="panel-body">
                                <h1 class="fs30 mt5 mbn">63,262</h1>
                                <h6 class="text-success">Attendees</h6>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-4 col-xl-3">
                        <div class="panel panel-tile text-center br-a br-grey">
                            <div class="panel-body">
                                <h1 class="fs30 mt5 mbn">$248000</h1>
                                <h6 class="text-warning">Revenue</h6>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-3 col-xl-3 visible-xl">
                        <div class="panel panel-tile text-center br-a br-grey">
                            <div class="panel-body">
                                <h1 class="fs30 mt5 mbn">{{ users|length }}</h1>
                                <h6 class="text-danger">Users</h6>
                            </div>
                        </div>
                    </div>
                </div>


                <!-- Pie and Circle Stats  -->
                <div class="row" id="pchart5">
                    <div class="col-md-5">
                        <!-- Pie Chart -->
                        <div class="panel">
                            <div class="panel-heading">
                                <span class="panel-title fw600 text-info">Pie Chart</span>
                            </div>
                            <div class="panel-body pn">
                                <div id="high-pie" style="width: 100%; height: 364px; margin: 0 auto"></div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-7">
                        <!-- Circle Stats -->
                        <div class="panel">
                            <div class="panel-heading">
                                <span class="panel-title fw600 text-info">Circulars</span>
                                <div class="widget-menu pull-right mr10 hidden">
                    <span class="fs11 text-muted">
                      <i class="fa fa-circle text-warning fs12 pr5"></i> Adult </span>
                                    <span class="fs11 text-muted ml10">
                      <i class="fa fa-circle text-info fs12 pr5"></i> University Student </span>
                                    <span class="fs11 text-muted ml10">
                      <i class="fa fa-circle text-primary fs12 pr5"></i> Youth </span>
                                </div>
                            </div>
                            <div class="panel-body">
                                <div class="mb20 text-right">
                    <span class="fs11 text-muted">
                      <i class="fa fa-circle text-warning fs12 pr5"></i> Youth</span>
                                    <span class="fs11 text-muted ml10">
                      <i class="fa fa-circle text-info fs12 pr5"></i> University Student</span>
                                    <span class="fs11 text-muted ml10">
                      <i class="fa fa-circle text-primary fs12 pr5"></i> Adult </span>
                                </div>
                                <div class="row">
                                    <div class="col-md-4 text-center">
                                        <div class="info-circle" id="c1" title="Adult" value="60" data-circle-color="primary"></div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="info-circle" id="c2" title="University Student" value="30" data-circle-color="info"></div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="info-circle" id="c3" title="Youth" value="10" data-circle-color="warning"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- recent activity table -->
                <div class="panel">
                    <div class="panel-heading">
                        <span class="panel-title hidden-xs"> Recent Activity</span>
                    </div>
                    <div class="panel-body pn">
                        <div class="table-responsive">
                            <table class="table admin-form theme-warning tc-checkbox-1 fs13">
                                <thead>
                                <tr class="bg-light">
                                    <th class="">Event</th>
                                    <th class="">Event Id</th>
                                    <th class="">Revenue</th>
                                    <th class="">Attendies</th>
                                    <th class="text-right">Status</th>

                                </tr>
                                </thead>
                                <tbody>
                                <tr>
                                    <td class="">Summer Camp</td>
                                    <td class="">21362</td>
                                    <td class="">$215</td>
                                    <td class="">1,400</td>
                                    <td class="text-right">
                                        <div class="btn-group text-right">
                                            <button type="button" class="btn btn-success br2 btn-xs fs12 dropdown-toggle" data-toggle="dropdown" aria-expanded="false"> Active
                                                <span class="caret ml5"></span>
                                            </button>
                                        </div>
                                    </td>
                                </tr>
                                <tr>

                                    <td class="">Winter Camp</td>
                                    <td class="">1262</td>
                                    <td class="">$4555</td>
                                    <td class="">100</td>
                                    <td class="text-right">
                                        <div class="btn-group text-right">
                                            <button type="button" class="btn btn-success br2 btn-xs fs12 dropdown-toggle" data-toggle="dropdown" aria-expanded="false"> Active
                                                <span class="caret ml5"></span>
                                            </button>
                                        </div>
                                    </td>
                                </tr>
                                <tr>

                                    <td class="">Thanks Giving Camp</td>
                                    <td class="">66362</td>
                                    <td class="">$1699</td>
                                    <td class="">100</td>
                                    <td class="text-right">
                                        <div class="btn-group text-right">
                                            <button type="button" class="btn btn-success br2 btn-xs fs12 dropdown-toggle" data-toggle="dropdown" aria-expanded="false"> Active
                                                <span class="caret ml5"></span>
                                            </button>
                                        </div>
                                    </td>
                                </tr>
                                <tr>

                                    <td class="">Spring Camp</td>
                                    <td class="">51362</td>
                                    <td class="">$1299</td>
                                    <td class="">5,200</td>
                                    <td class="text-right">
                                        <div class="btn-group text-right">
                                            <button type="button" class="btn btn-success br2 btn-xs fs12 dropdown-toggle" data-toggle="dropdown" aria-expanded="false"> Active
                                                <span class="caret ml5"></span>
                                            </button>
                                            <ul class="dropdown-menu" role="menu">
                                                <li>
                                                    <a href="#">Edit</a>
                                                </li>
                                                <li>
                                                    <a href="#">Delete</a>
                                                </li>
                                                <li>
                                                    <a href="#">Archive</a>
                                                </li>
                                                <li class="divider"></li>
                                                <li>
                                                    <a href="#">Complete</a>
                                                </li>
                                                <li class="active">
                                                    <a href="#">Pending</a>
                                                </li>
                                                <li>
                                                    <a href="#">Canceled</a>
                                                </li>
                                            </ul>
                                        </div>
                                    </td>
                                </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>

                <!-- info traffic panels -->
                <div class="row">
                    <div class="col-md-12">
                        <div class="panel">
                            <div class="panel-heading">
                                <span class="panel-title">Top Geographic Sources</span>
                            </div>
                            <div class="panel-body pn">
                                <table class="table mbn tc-med-1 tc-bold-last">
                                    <thead>
                                    <tr class="">
                                        <th>State</th>
                                        <th>Male</th>
                                        <th>Female</th>
                                        <th>Youth</th>
                                        <th>Adult</th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <tr>
                                        <td>
                                            <span class="flag-xs flag-us mr5 va-b"></span>Texas</td>
                                        <td>28%</td>
                                        <td>2%</td>
                                        <td>12%</td>
                                        <td>8%</td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <span class="flag-xs flag-de mr5 va-b"></span>California</td>
                                        <td>12%</td>
                                        <td>5%</td>
                                        <td>2%</td>
                                        <td>19%</td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <span class="flag-xs flag-fr mr5 va-b"></span>Florida</td>
                                        <td>14%</td>
                                        <td>15%</td>
                                        <td>12%</td>
                                        <td>29%</td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <span class="flag-xs flag-tr mr5 va-b"></span>New Jersey</td>
                                        <td>22%</td>
                                        <td>11%</td>
                                        <td>21%</td>
                                        <td>19%</td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <span class="flag-xs flag-es mr5 va-b"></span>Turkey</td>
                                        <td>33%</td>
                                        <td>23%</td>
                                        <td>18%</td>
                                        <td>12%</td>
                                    </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <!-- end: .tray-center -->



        </section>
        <!-- End: Content -->

    </section>
    <!-- END: PAGE SCRIPTS -->
    </body>
    <!-- End: Content -->
{% endblock %}
{% block javascripts %}
    <!-- jQuery -->
    <script src="{{ asset('assets/vendor/jquery/jquery-1.11.1.min.js') }}"></script>
    <script src="{{ asset('assets/vendor/jquery/jquery_ui/jquery-ui.min.js') }}"></script>

    <!-- Sparkline Local -->
    <script src="{{ asset('assets/vendor/plugins/sparkline/jquery.sparkline.min.js') }}"></script>

    <!-- HighCharts Plugin -->
    <script src="{{ asset('assets/vendor/plugins/highcharts/highcharts.js') }}"></script>
    <script src="{{ asset('assets/vendor/plugins/circles/circles.js') }}"></script>

    <!-- Theme Javascript -->
    <script src="{{ asset('assets/js/utility/utility.js') }}"></script>
    <script src="{{ asset('assets/js/demo/demo.js') }}"></script>
    <script src="{{ asset('assets/js/main.js') }}"></script>
    <script src="{{ asset('assets/js/demo/charts/highcharts.js') }}"></script>
    <script type="text/javascript">
        jQuery(document).ready(function() {

            "use strict";

            // Init Theme Core
            Core.init();

            // Init Demo JS
            Demo.init();

            // This page contains more Initilization Javascript than normal.
            // As a result it has its own js page. See charts.js for more info
            demoHighCharts.init();

        });
    </script>
{% endblock %}