<header class="lavel-up-header lavel-up-header-bg-img" data-stellar-background-ratio="0.5">
    <div class="lavel-up-color-overlay">
        <div class="container text-center">

            <!-- Logo start -->

            <!-- Logo End -->
            <!-- Main title end -->
            <!-- Start: Main -->
            <!-- Start: Content-Wrapper -->
            <section id="content_wrapper">

                <!-- Begin: Content -->
                <section id="content">
                    <div style="width:100%;   padding-top: 0em; ">

                        <div class="admin-form theme-info mw400" id="login" style="position: relative;  top: 10em; left: -5em;">

                        </div>
                        <div class="admin-form theme-info mw400" id="login" style="position: relative; left: 37em; bottom: -6em;  ">

                            <!-- Login Logo -->
                            <div class="row table-layout">
                                <a href="dashboard.html" title="Return to Dashboard"></a>
                            </div>

                            <form method="post" action="{{ path('register') }}" id="admin-form">

                                <div class="panel-body bg-light p25 pb15" style="background-color: rgba(255,255,255,0.3) !important; border-radius: 0.5em">
                                    <div style="font-size:22px; margin-bottom:17px; text-align: left; color: #f7f8e6;">Sign Up
                                        {% if not form.username.vars.valid %}
                                            <div class="alert alert-danger alert-dismissable">
                                                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                                                <i class="fa fa-remove pr10"></i>

                                                <strong>{{ form_errors(form.username) }}</strong>
                                            </div>
                                        {% endif %}
                                        {% if not form.plainPassword.vars.valid %}
                                            <div class="alert alert-danger alert-dismissable">
                                                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                                                <i class="fa fa-remove pr10"></i>
                                                <strong> {{ form_errors(form.plainPassword) }}</strong>
                                            </div>
                                        {% endif %}
                                        {% if error %}
                                            <div class="alert alert-danger">{{ error.messageKey|trans(error.messageData, 'security') }}</div>
                                        {% endif %}
                                    </div>
                                    <div class="section">
                                        <label for="username" class="field prepend-icon">
                                            {{ form_widget(form.fullname, {'attr': {'class': 'gui-input', 'placeholder':'Fullname...', 'type':'text', 'id':"firstname" }}) }}
                                            <label for="username" class="field-icon">
                                                <i class="fa fa-user"></i>
                                            </label>
                                        </label>
                                    </div>
                                    <div class="section">
                                        <label for="password" class="field prepend-icon">
                                            {{ form_widget(form.username, {'attr': {'class': 'gui-input', 'placeholder':'Email address', 'type':'email', 'id':'email', 'name':'_username'}}) }}
                                            <label for="password" class="field-icon">
                                                <i class="fa fa-envelope"></i>
                                            </label>
                                        </label>
                                    </div>
                                    <div class="section">
                                        <label for="password" class="field prepend-icon">
                                            {{ form_widget(form.plainPassword.first, {'attr': {'class': 'gui-input', 'placeholder':'Create a password', 'type':'password', 'id':'password'}}) }}
                                            <label for="password" class="field-icon">
                                                <i class="fa fa-lock"></i>
                                            </label>
                                        </label>
                                    </div>
                                    <div class="section">
                                        <label for="password" class="field prepend-icon">
                                            {{ form_widget(form.plainPassword.second, {'attr': {'class': 'gui-input', 'placeholder':'Retype your password', 'id':'confirmPassword', 'type':'password'}}) }}                                            <label for="password" class="field-icon">
                                                <i class="fa fa-lock"></i>
                                            </label>
                                        </label>
                                    </div>
                                    {{ form_row(form._token) }}
                                    <!-- end .section row section -->
                                </div>

                                <div class="panel-footer clearfix" style="background-color: rgba(255, 255, 255, 0.5) !important; border-radius: 0.5em">
                                    <button type="submit" class="button btn-primary mr10 pull-right">Sign Up</button>

                                </div>
                            </form>
